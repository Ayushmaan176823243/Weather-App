{
  "expo": {
    "name": "Nimbus",
    "slug": "my-expo-app",
    "version": "1.0.0",

    "web": {
      "favicon": "./assets/weather.png"
    },

    "experiments": {
      "tsconfigPaths": true
    },

    "plugins": [
      [
        "expo-dev-launcher",
        {
          "launchMode": "most-recent"
        }
      ]
    ],

    "orientation": "portrait",
    "icon": "./assets/weather.png",

    "userInterfaceStyle": "light",

    "splash": {
      "image": "./assets/weather.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/weather.png",
        "backgroundColor": "#ffffff"
      }
    }
  }
}
